package net.wanho.po.product;

import java.util.Date;

/**
 * 流程 封装类
 * @author Administrator
 *
 */
public class ProcessInfo {
	/**流程编码*/
	private Integer processId;
	/**流程类型*/
	private String type;
	/**操作对象编码*/
	private Integer businessId;
	/**操作状态*/
	private String status;
	/**意见*/
	private String  opinions;
	/**操作人*/
	private Integer operator;
	/**流程时间*/
	private Date createTime;
	
	
	public ProcessInfo() {
		super();
	}
	
	public ProcessInfo(Integer processId, String type, Integer businessId, String status, String opinions, Integer operator,
			Date createTime) {
		super();
		this.processId = processId;
		this.type = type;
		this.businessId = businessId;
		this.status = status;
		this.opinions = opinions;
		this.operator = operator;
		this.createTime = createTime;
	}

	public Integer getProcessId() {
		return processId;
	}
	public void setProcessId(Integer processId) {
		this.processId = processId;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public Integer getBusinessId() {
		return businessId;
	}
	public void setBusinessId(Integer businessId) {
		this.businessId = businessId;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getOpinions() {
		return opinions;
	}
	public void setOpinions(String opinions) {
		this.opinions = opinions;
	}
	public Integer getOperator() {
		return operator;
	}
	public void setOperator(Integer operator) {
		this.operator = operator;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	@Override
	public String toString() {
		return "Process [processId=" + processId + ", type=" + type + ", businessId=" + businessId + ", status="
				+ status + ", opinions=" + opinions + ", operator=" + operator + ", createTime=" + createTime + "]";
	}
	
	
	
	
}
