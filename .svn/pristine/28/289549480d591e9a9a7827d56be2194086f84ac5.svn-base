package net.wanho.po.product;

import java.math.BigDecimal;
import java.util.Date;

/**
 * 产品 封装类
 * @author Administrator
 *
 */
public class Product {
	/**产品编码*/
	private Integer productId;
	/**产品类型*/
	private Integer prodType;
	/**产品名称*/
	private String prodName;
	/**产品规格*/
	private String prodStandard;
	/**成本价*/
	private BigDecimal costPrice;
	/**零售价*/
	private BigDecimal retailPrice;
	/**备注*/
	private String  remarks;
	/**状态*/
	private String status;
	/**创建时间*/
	private Date createTime;
	/**更新时间*/
	private Date updateTime;
	
	
	
	public Product() {
		super();
	}

	
	public Product(Integer productId, Integer prodType, String prodName, String prodStandard, BigDecimal costPrice,
			BigDecimal retailPrice, String remarks, String status, Date createTime, Date updateTime) {
		super();
		this.productId = productId;
		this.prodType = prodType;
		this.prodName = prodName;
		this.prodStandard = prodStandard;
		this.costPrice = costPrice;
		this.retailPrice = retailPrice;
		this.remarks = remarks;
		this.status = status;
		this.createTime = createTime;
		this.updateTime = updateTime;
	}


	public Integer getProductId() {
		return productId;
	}


	public void setProductId(Integer productId) {
		this.productId = productId;
	}


	public Integer getProdType() {
		return prodType;
	}


	public void setProdType(Integer prodType) {
		this.prodType = prodType;
	}


	public String getProdName() {
		return prodName;
	}


	public void setProdName(String prodName) {
		this.prodName = prodName;
	}


	public String getProdStandard() {
		return prodStandard;
	}


	public void setProdStandard(String prodStandard) {
		this.prodStandard = prodStandard;
	}


	public BigDecimal getCostPrice() {
		return costPrice;
	}


	public void setCostPrice(BigDecimal costPrice) {
		this.costPrice = costPrice;
	}


	public BigDecimal getRetailPrice() {
		return retailPrice;
	}


	public void setRetailPrice(BigDecimal retailPrice) {
		this.retailPrice = retailPrice;
	}


	public String getRemarks() {
		return remarks;
	}


	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}


	public String getStatus() {
		return status;
	}


	public void setStatus(String status) {
		this.status = status;
	}


	public Date getCreateTime() {
		return createTime;
	}


	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}


	public Date getUpdateTime() {
		return updateTime;
	}


	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}


	@Override
	public String toString() {
		return "Product [productId=" + productId + ", prodType=" + prodType + ", prodName=" + prodName
				+ ", prodStandard=" + prodStandard + ", costPrice=" + costPrice + ", retailPrice=" + retailPrice
				+ ", remarks=" + remarks + ", status=" + status + ", createTime=" + createTime + ", updateTime="
				+ updateTime + "]";
	}
	
	
	
}
