package net.wanho.po.person;

import java.util.Date;

/**
 * 个人任务
 * @author Administrator
 *
 */
public class IndividualTask {
	/**任务编码*/
	private Integer  taskId;
	/**任务主题*/
	private String topic;
	/**关联信息*/
	private String description;
	/**负责人*/
	private Integer responsiblePerson;
	/**结束时间*/
	private Date endTime;
	/**优先级*/
	private String priority;
	/**状态*/
	private String status;
	/**创建时间*/
	private Date createTime;
	/**操作人*/
	private Integer operator;
	/**相关链接*/
	private String linkUrl;
	/**任务类型*/
	private String taskType;
	public IndividualTask() {
		super();
		// TODO Auto-generated constructor stub
	}
	public IndividualTask(Integer taskId, String topic, String description, Integer responsiblePerson, Date endTime,
			String priority, String status, Date createTime, Integer operator, String linkUrl, String taskType) {
		super();
		this.taskId = taskId;
		this.topic = topic;
		this.description = description;
		this.responsiblePerson = responsiblePerson;
		this.endTime = endTime;
		this.priority = priority;
		this.status = status;
		this.createTime = createTime;
		this.operator = operator;
		this.linkUrl = linkUrl;
		this.taskType = taskType;
	}
	public Integer getTaskId() {
		return taskId;
	}
	public void setTaskId(Integer taskId) {
		this.taskId = taskId;
	}
	public String getTopic() {
		return topic;
	}
	public void setTopic(String topic) {
		this.topic = topic;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public Integer getResponsiblePerson() {
		return responsiblePerson;
	}
	public void setResponsiblePerson(Integer responsiblePerson) {
		this.responsiblePerson = responsiblePerson;
	}
	public Date getEndTime() {
		return endTime;
	}
	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}
	public String getPriority() {
		return priority;
	}
	public void setPriority(String priority) {
		this.priority = priority;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Integer getOperator() {
		return operator;
	}
	public void setOperator(Integer operator) {
		this.operator = operator;
	}
	public String getLinkUrl() {
		return linkUrl;
	}
	public void setLinkUrl(String linkUrl) {
		this.linkUrl = linkUrl;
	}
	public String getTaskType() {
		return taskType;
	}
	public void setTaskType(String taskType) {
		this.taskType = taskType;
	}
	@Override
	public String toString() {
		return "IndividualTask [taskId=" + taskId + ", topic=" + topic + ", description=" + description
				+ ", responsiblePerson=" + responsiblePerson + ", endTime=" + endTime + ", priority=" + priority
				+ ", status=" + status + ", createTime=" + createTime + ", operator=" + operator + ", linkUrl="
				+ linkUrl + ", taskType=" + taskType + "]";
	}
	
	
}
